<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Pascal Berger (BBT Software AG) --><!-- edited with XMLSpy v2008 (http://www.altova.com) by . (.) --><!-- XML-Definition für Dokumentenübermittlung--><!-- Kommentare bitte an christian.treyer@bbtsoftware.ch --><!--  --><!-- Copyright (R) 2010 by BBT Software AG --><!-- Dieses Schema darf nur für den Transfer von Dokumenten an Versicherer verwendet werden, die im Besitz einer Sunetplus-Lizenz sind.--><!-- Eine andersweitige Verwendung ist möglich, muss aber unter Angabe des Verwendungszwecks von BBT Software AG bewilligt werden. --><!--  --><xs:schema targetNamespace="http://www.sunet.ch/schema/attachmentRequest/1.0_DRAFT1" xmlns:cr="http://www.sunet.ch/schema/attachmentRequest/1.0_DRAFT1" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1">	<!-- the root tag is "attachmentRequest" -->	<xs:element name="attachmentRequest">		<xs:annotation>			<xs:documentation xml:lang="de">Dokumentenübermittlung</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:choice maxOccurs="unbounded">					<xs:element ref="cr:attachment"/>				</xs:choice>			</xs:sequence>			<xs:attribute name="sourceApplication" type="xs:string" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">Absendende Applikation.</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="sourceApplicationVersion" type="cr:stype.applicationVersion" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">Version der absendenden Applikation.</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="destinationInsuranceCompany" type="xs:string" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">Empfänger(-Versicherung).</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="schemaVersion" type="xs:decimal" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">Schema-Version.</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="testRequest" type="xs:boolean" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">Flag ob Übermittlung in Testsystem.</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="ignoreRequest" type="xs:boolean" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">Flag ob übermittelte Dokumente verarbeitet werden sollen oder nicht.</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="attachment">		<xs:annotation>			<xs:documentation xml:lang="de">Ein einzelnes Dokument</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="identity" type="cr:ctype.identity">					<xs:annotation>						<xs:documentation xml:lang="de">Angaben zur Identifikation des Schadenmeldungsfalles</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="contentType">					<xs:annotation>						<xs:documentation xml:lang="de">Datei Typ</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="PDF"/>							<xs:enumeration value="JPEG"/>							<xs:enumeration value="TIFF"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="fileName">					<xs:annotation>						<xs:documentation xml:lang="de">Ursprünglicher Dateiname</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="255"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="contentBase64" type="xs:base64Binary">					<xs:annotation>						<xs:documentation xml:lang="de">Base64 codiertes Dokument</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="submittingPerson" type="cr:ctype.submittingPerson" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Übermittelnde Person</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="remark" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Bemerkungen</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="255"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<!--Complex Datatypes -->	<xs:complexType name="ctype.submittingPerson">		<xs:annotation>			<xs:documentation xml:lang="de">Kontaktperson</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="name" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Name der übermittelnden Person</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="phone" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Telefon-Nr. der übermittelnden Person</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="30"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="email" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">E-Mail Adresse der übermittelnden Person</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ctype.identity">		<xs:annotation>			<xs:documentation xml:lang="de">Personen-Identifikation</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="caseUUID" type="cr:stype.UUID">				<xs:annotation>					<xs:documentation xml:lang="de">Externe Fallidentifikation</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="claimNumber" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Schadennummer</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string"/>				</xs:simpleType>			</xs:element>			<xs:element name="policyNumber" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Policen-Nr. der Originalmeldung</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string"/>				</xs:simpleType>			</xs:element>			<xs:element name="firstname" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Vorname der Person der Originalmeldung</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="25"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="lastname" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Nachname der Person der Originalmeldung</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="25"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="dateOfBirth" type="xs:date" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Geburtsdatum der Person der Originalmeldung</xs:documentation>				</xs:annotation>			</xs:element>			<xs:choice>				<xs:element name="AHVnumber" type="cr:stype.AHVnumber" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">AHV Nummer der Person der Originalmeldung, falls sie eine AHV Nummer besitzt</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="SVnumber" type="cr:stype.SVnumber" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">SV Nummer der Person der Originalmeldung, falls sie eine SV Nummer besitzt</xs:documentation>					</xs:annotation>				</xs:element>			</xs:choice>		</xs:sequence>	</xs:complexType>	<!--Simple Datatypes-->	<xs:simpleType name="stype.AHVnumber">		<xs:annotation>			<xs:documentation xml:lang="de">AHV-Nummer</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="[0-9]{3}.[0-9]{2}.[0-9]{3}.[0-9]{3}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="stype.applicationVersion">		<xs:annotation>			<xs:documentation xml:lang="de">Versionsnummer / Revisionnummer</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:integer">			<xs:pattern value="[0-9]{4}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="stype.SVnumber">		<xs:annotation>			<xs:documentation xml:lang="de">Sozialversicherungs-Nummer bzw. "Neue AHV-Nummer"</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="[0-9]{3}\.[0-9]{4}\.[0-9]{4}\.[0-9]{2}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="stype.UUID">		<xs:annotation>			<xs:documentation xml:lang="de">UUID</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:length value="36"/>			<xs:pattern value="([0-9]|[a-f]|[A-F]){8}(-([0-9]|[a-f]|[A-F]){4})(-([0-9]|[a-f]|[A-F]){4})(-([0-9]|[a-f]|[A-F]){4})(-([0-9]|[a-f]|[A-F]){12})"/>		</xs:restriction>	</xs:simpleType></xs:schema>