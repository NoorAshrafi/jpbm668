<?xml version="1.0" encoding="UTF-8"?><!-- 11.12.2012, chkdac, Update auf Subversion 1.7_V8 --><!-- XML-Definition für die Übermittlung von UVG und KTG Schadenmeldungen --><!-- Kommentare bitte an christian.treyer@bbtsoftware.ch --><!--  --><!-- Copyright (c) 2004-2012 by Suva (Schweizerische Unfallversicherungsanstalt) --><!-- Dieses Schema darf nur für den Datentransfer zu Versicherungen verwendet werden, die Inhaber einer Sunet-Lizenz sind. --><!-- Eine andersweitige Verwendung muss unter Angabe der betroffenen Applikation/Versicherung von der Suva bewilligt werden. --><!--  --><!--  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cr="http://www.sunet.ch/schema/claimReport/1.7" targetNamespace="http://www.sunet.ch/schema/claimReport/1.7" elementFormDefault="qualified" attributeFormDefault="unqualified" version="8"> --><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cr="http://www.sunet.ch/schema/claimReport/1.7" targetNamespace="http://www.sunet.ch/schema/claimReport/1.7" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1">	<!-- the root tag is "claimReport" -->	<xs:element name="claimReport">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Schadenmeldung</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:choice maxOccurs="16">					<xs:element ref="cr:accidentReportUVG"/>					<xs:element ref="cr:illnessReport"/>				</xs:choice>			</xs:sequence>			<xs:attribute name="sourceApplication" type="xs:string" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Absendende Applikation</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="sourceApplicationVersion" type="cr:stype.applicationVersion" use="required"/>			<xs:attribute name="destinationInsuranceCompany" type="xs:string" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Empfänger(-Versicherung)</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="schemaVersion" type="xs:decimal" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Schema-Version</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="testClaimReport" type="xs:boolean" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Test-XML-File Ja/Nein</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="accidentReportUVG">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Unfallmeldung</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:choice>					<xs:element ref="cr:regularAccidentReportUVG"/>					<xs:element ref="cr:minorAccidentReportUVG"/>				</xs:choice>			</xs:sequence>			<xs:attribute name="caseUUID" type="cr:stype.UUID" use="required"/>			<xs:attribute name="secondReport" type="xs:boolean" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>zweites Mal übermittelt</translation>						<description>							<short>Dieses Flag wird auf True gesetzt, wenn der Fall ein zweites mal übermittelt wird</short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="dateAndTimeOfReport" type="xs:dateTime" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Datum und Zeit der Schadenmeldung</translation>						<description>							<short>Dieses Attribut entspricht dem Datum der Schadenmeldung, siehe unten auf dem Formular (Ort, Datum, Unterschrift) </short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="cityWhereFiled" type="xs:string" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Ort, wo die Schadenmeldung erstellt wurde</translation>						<description>							<short>Dieses Attribut entspricht dem Ort, an dem die Schadenmeldung erstellt wurde, siehe unten auf dem Formular (Ort, Datum, Unterschrift) </short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="submitDateAndTime" type="xs:dateTime" use="optional">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Übermittlungsdatum und -zeit</translation>						<description>							<short>Dieses Attribut wird für die Übermittlung zur Versicherung nicht verwendet. Es kann NACH einer erfolgreichen Übermittlung für eine weitere Verarbeitung verwendet werden.</short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="regularAccidentReportUVG">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Unfallmeldung (nicht-Bagatell)</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="employer" type="cr:ctype.employer"/>				<xs:element name="person" type="cr:ctype.person"/>				<xs:element name="employment" type="cr:ctype.employment"/>				<xs:element name="paymentAddress" minOccurs="0">					<xs:complexType>						<xs:complexContent>							<xs:extension base="cr:ctype.paymentAddress"/>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:element name="accident" type="cr:ctype.accident"/>				<xs:element name="incapacityToWork" type="cr:ctype.incapacityToWork" minOccurs="0"/>				<xs:element name="policyDataUVG" type="cr:ctype.policyDataUVG"/>				<xs:choice>					<xs:element name="additionsBU" type="cr:ctype.additionsBU"/>					<xs:element name="additionsNBU" type="cr:ctype.additionsNBU"/>				</xs:choice>				<xs:element name="salaryData" type="cr:ctype.salaryData"/>				<xs:element name="dcmData" type="cr:ctype.dcmData" minOccurs="0"/>			</xs:sequence>			<xs:attribute name="accidentCategory" type="cr:stype.accidentCategory" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="minorAccidentReportUVG">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Bagatellunfallmeldung</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="employer" type="cr:ctype.employer"/>				<xs:element name="person" type="cr:ctype.person"/>				<xs:element name="employment" type="cr:ctype.employment"/>				<xs:element name="paymentAddress" type="cr:ctype.paymentAddress" minOccurs="0"/>				<xs:element name="accident" type="cr:ctype.accident"/>				<xs:element name="policyDataUVG" type="cr:ctype.policyDataUVG"/>				<xs:choice>					<xs:element name="additionsBU" type="cr:ctype.additionsBU"/>					<xs:element name="additionsNBU" type="cr:ctype.additionsNBU"/>				</xs:choice>				<xs:element name="dcmData" type="cr:ctype.dcmData" minOccurs="0"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="illnessReport">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Krankmeldung</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="employer" type="cr:ctype.employer"/>				<xs:element name="person" type="cr:ctype.person"/>				<xs:element name="employment" type="cr:ctype.employment"/>				<xs:element name="paymentAddress" type="cr:ctype.paymentAddress" minOccurs="0"/>				<xs:element name="policyDataKTG" type="cr:ctype.policyDataKTG"/>				<xs:choice>					<xs:element name="illness" type="cr:ctype.illness"/>					<xs:element name="maternity" type="cr:ctype.maternity"/>				</xs:choice>				<xs:element name="salaryData" type="cr:ctype.salaryData"/>				<xs:element name="dcmData" type="cr:ctype.dcmData" minOccurs="0"/>			</xs:sequence>			<xs:attribute name="caseUUID" type="cr:stype.UUID" use="required"/>			<xs:attribute name="secondReport" type="xs:boolean" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>zweites Mal übermittelt</translation>						<description>							<short>Dieses Flag wird auf True gesetzt, wenn der Fall ein zweites mal übermittelt wird</short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="dateAndTimeOfReport" type="xs:dateTime" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Datum und Zeit der Schadenmeldung</translation>						<description>							<short>Dieses Attribut entspricht dem Datum der Schadenmeldung, siehe unten auf dem Formular (Ort, Datum, Unterschrift) </short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="cityWhereFiled" type="xs:string" use="required">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Ort, wo die Schadenmeldung erstellt wurde</translation>						<description>							<short>Dieses Attribut entspricht dem Ort, an dem die Schadenmeldung erstellt wurde, siehe unten auf dem Formular (Ort, Datum, Unterschrift) </short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="submitDateAndTime" type="xs:dateTime" use="optional">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Übermittlungsdatum und -zeit</translation>						<description>							<short>Dieses Attribut wird für die Übermittlung zur Versicherung nicht verwendet. Es kann NACH einer erfolgreichen Übermittlung für eine weitere Verarbeitung verwendet werden.</short>						</description>					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<!--Complex Datatypes -->	<xs:complexType name="ctype.additionsBU">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Zusatzdaten BU-Fall</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="BUinvolvedObjects" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="100"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ctype.additionsNBU">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Zusatzdaten NBU-Fall</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="lastWorkingDayDate" type="xs:date" minOccurs="0"/>			<xs:element name="lastWorkingDayTime" type="xs:time" minOccurs="0"/>			<xs:element name="reasonForAbsence" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="40"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="activityAtTimeOfAccident" type="cr:stype.activityAtTimeOfAccident"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ctype.accident">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Unfalldaten</translation>				<description>					<short>Unfalldaten</short>				</description>			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="accidentDate" type="xs:date"/>			<xs:element name="accidentTime" type="xs:time" minOccurs="0"/>			<xs:element name="relapseDate" type="xs:date" minOccurs="0"/>			<xs:element name="unpreciseAccidentDate" type="xs:boolean"/>			<xs:element name="placeOfAccident">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="40"/>					</xs:restriction>				</xs:simpleType>				<!--UnfallOrt-->			</xs:element>			<xs:element name="sceneOfAccident" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="70"/>					</xs:restriction>				</xs:simpleType>				<!--Unfallstelle-->			</xs:element>			<xs:element name="descriptionOfAccident">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="3900"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="policeReportExists" type="cr:stype.policeReportExists" minOccurs="0"/>			<xs:element name="involvedPersons" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="100"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="moreThan1MonthIncapacityForWork" type="xs:boolean" minOccurs="0"/>			<xs:element name="injury" type="cr:ctype.injury" maxOccurs="3"/>			<xs:element name="treatment" type="cr:ctype.treatment"/>		</xs:sequence>		<xs:attribute name="relapse" type="xs:boolean" use="required">			<xs:annotation>				<xs:documentation xml:lang="de">					<translation>Rückfall</translation>					<description>						<short>Unfalldaten</short>					</description>				</xs:documentation>			</xs:annotation>		</xs:attribute>	</xs:complexType>	<xs:complexType name="ctype.bankAccount">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Kontodaten</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="accountHolder" type="cr:stype.accountHolder"/>			<xs:element name="accountNumber" type="xs:string"/>			<xs:choice>				<xs:element name="postalAccountOfBank" type="xs:string"/>				<xs:element name="clearingNumberOfBank" type="xs:string"/>			</xs:choice>			<xs:element name="bankAddress" type="cr:ctype.companyAddress"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ctype.companyAddress">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Firmenadresse</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="name1">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="name2" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="address1" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="address2" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="plz">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="9"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="city">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="country" type="cr:stype.nations" minOccurs="0"/>			<xs:element name="phone" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="15"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="email" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="30"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ctype.contactPerson">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>Kontaktperson</translation>				<description>					<short/>				</description>			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="name">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="phone" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="30"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="fax" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="30"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="email" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ctype.dcmData">		<xs:annotation>			<xs:documentation xml:lang="de">				<translation>DCM-Daten</translation>				<description>					<short>Zusatzdaten für DCM</short>				</description>			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="activityPutsStrainOn" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Die Tätigkeit belastet</translation>						<description>							<short>Code 1: Rücken // Code 2: Schulter // Code 3: Rücken und Schulter // Code 4: keine Rücken- und Schulterbelastung</short>						</description>					</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:integer">						<xs:enumeration value="1"/>						<xs:enumeration value="2"/>						<xs:enumeration value="3"/>						<xs:enumeration value="4"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="additionalOperatingExpenses" type="xs:decimal" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Betriebliche Mehraufwendungen</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="additionalPhoneNumber" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>zusätzliche Telefonnummer</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="allocationOfWorkload" type="cr:ctype.dcm.allocationOfWorkload" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Prozentuale Aufteilung der Tätigkeit</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="bvgInsurer" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>BVG-Versicherer</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="bvgInsurerNotified" type="xs:boolean" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Wurde die BVG-Versicherung informiert? Ja/Nein</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="bvgInsurerNotifiedDate" type="xs:date" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Datum, wann der BVG-Versicherer informiert wurde</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="bvgInsurerPolicyNumber" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Policen-Nummer beim BVG-Versicherer</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="dateOfFirstConsultation" type="xs:date" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Datum erster Arztbesuch</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="fixedOperatingCost" type="xs:decimal" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Fixe Betriebskosten</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="healingCostInsurer" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Heilungskostenversicherer / obligatorische Krankenversicherung KVG</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="incomeDuringUnemployment" type="xs:boolean" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Im Zwischenverdienst</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="lastWorkingDay" type="xs:date" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">						<translation>Letzter Arbeitstag</translation>						<description>							<short/>						</description>					</xs:documentation>				</xs:annotation>			</xs:element>	