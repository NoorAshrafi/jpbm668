					<simpleType>
						<restriction base="NMTOKEN">
							<enumeration value="A"/>
							<enumeration value="B"/>
							<enumeration value="C"/>
							<enumeration value="D"/>
							<enumeration value="E"/>
						</restriction>
					</simpleType>
				</attribute>
				<attribute name="delivery" default="first">
					<simpleType>
						<restriction base="NMTOKEN">
							<enumeration value="first"/>
							<enumeration value="repeated"/>
							<enumeration value="permanent"/>
						</restriction>
					</simpleType>
				</attribute>
				<attribute name="regulation">
					<simpleType>
						<restriction base="string">
							<pattern value="[1-9][0-9]{2}"/>
						</restriction>
					</simpleType>
				</attribute>
				<attribute name="limitation" type="boolean" default="false"/>
				<attribute name="remark" type="invoice:stringType1_350"/>
			</extension>
		</simpleContent>
	</complexType>
